{
  "expo": {
    "name": "LedFx",
    "slug": "LedFx",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "primaryColor": "#333333",
    "description": "Control your LedFx sound reactive LEDs from your phone!",
    "privacy": "public",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#333333"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/white-icon.png",
        "backgroundColor": "#333333"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
